		/* Paragraph */
		{
			"selector": ".entry-content p, #respond p",
			"type"    : "text",
			"label"   : "Paragraph"
		},
		{
			"selector": ".entry-content p, #respond p",
			"type"    : "color",
			"label"   : "Paragraph color"
		},
		/* Links set for content and comments. Comment selection should override */
		{
			"selector": ".entry-content a, #commentform a",
			"type"    : "color",
			"label"   : "Link color"
		},
		{
			"selector": ".entry-content a:hover, #commentform a:hover",
			"type"    : "color",
			"label"   : "Link hover color"
		},
		{
			"selector": ".entry-content pre",
