			"label"   : "Preformatted"
		},
		{
			"selector": ".entry-content pre",
			"type"    : "color",
			"label"   : "Preformatted color"
		},
		{
			"selector": ".entry-content pre",
			"type"    : "background-color",
			"label"   : "Preformatted background color"
		},
		{
			"selector": ".entry-content pre",
			"type"    : "color",
			"label"   : "Preformatted border color",
			"template": "%s { border-color: %s; }"
		},
		{
